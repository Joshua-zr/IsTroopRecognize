<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:background="@color/white"
             tools:context="com.istroop.istrooprecognize.ui.activity.Main2Activity">

	<FrameLayout
		android:id="@+id/fl_main_frame"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_marginBottom="50dp"/>

	<ImageView
		android:id="@+id/iv_reco"
		android:layout_width="wrap_content"
		android:layout_height="50dp"
		android:layout_gravity="bottom"
		android:contentDescription="@string/scan"
		android:src="@drawable/tab_reco_press"/>

	<ImageView
		android:id="@+id/iv_his"
		android:layout_width="wrap_content"
		android:layout_height="50dp"
		android:layout_gravity="bottom|end"
		android:contentDescription="@string/his_title"
		android:src="@drawable/tab_his_disabled"/>

</FrameLayout>
