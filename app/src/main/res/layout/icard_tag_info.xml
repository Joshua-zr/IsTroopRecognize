<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_white"
    android:orientation="vertical" >
    
    <RelativeLayout
        android:background="@color/white"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/icard_top" >

         <RelativeLayout
            android:clickable="true"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/icard_text_padding">
         <ImageView
           android:layout_width="10dip"
           android:layout_height="10dip"
           android:layout_centerVertical="true"
           android:src="@drawable/icard_arrow_left"
           android:layout_alignParentLeft="true"
           android:layout_marginLeft="@dimen/icard_left_right_little"
           />
        <TextView
            android:clickable="true"
            android:id="@+id/icard_tag_info_cannel"
            android:layout_marginLeft="@dimen/icard_left_right_big"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/design_headimage_quit"
            android:textSize="@dimen/icard_text_size_middle"
            android:textColor="@color/orange"/>
        <TextView
            android:visibility="gone"
            android:id="@+id/icard_login_menu"
            android:clickable="true"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:background="@drawable/menu_icon"/>
         </RelativeLayout>

        <RelativeLayout
            android:clickable="true"
            android:paddingRight="@dimen/icard_text_padding"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_width="150dip"
            android:layout_height="wrap_content">
        <TextView
            android:id="@+id/icard_tag_info_save"
            android:layout_width="100dip"
            android:gravity="right"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="@string/save"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_alignParentRight="true"
            android:textColor="@color/orange" />
        </RelativeLayout>
        <TextView
            android:id="@+id/icard_tag_info_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="@dimen/icard_text_size_big"
            android:layout_centerInParent="true" />

    </RelativeLayout>
    
     <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/icard_line"/>
    
    
     <ImageView
         android:layout_marginTop="10dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/icard_line"/>
           <RelativeLayout
           android:visibility="visible"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="@dimen/icard_top"
            android:paddingLeft="@dimen/icard_text_padding"
            android:background="@color/white"
            android:id="@+id/design_tag_link_info">
        <TextView
            android:layout_marginLeft="@dimen/icard_gv_spacing"
            android:clickable="true"
            android:id="@+id/design_tag_link_exp_info_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/design_tag_link"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_centerVertical="true"
            android:textColor="@color/black"/>
        <EditText
            android:textColorHint="@color/gray"
            android:background="@null"
            android:layout_marginLeft="20dip"
            android:layout_toRightOf="@+id/design_tag_link_exp_info_tv"
            android:id="@+id/design_tag_link_exp_info"
            android:ellipsize="end"
            android:singleLine="true"
            android:paddingRight="1dip"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_centerVertical="true"
            android:textSize="@dimen/icard_text_size_middle"/>
           
         </RelativeLayout>
        <ImageView
            android:visibility="visible"
            android:id="@+id/icard_tag_line2_info"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/icard_line"/>
       <RelativeLayout
           android:visibility="visible"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="@dimen/icard_top"
            android:paddingLeft="@dimen/icard_text_padding"
            android:background="@color/white"
            android:id="@+id/design_tag_title_info">
        <TextView
            android:layout_marginLeft="@dimen/icard_gv_spacing"
            android:clickable="true"
            android:id="@+id/design_tag_title_tv_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/design_tag_title"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_centerVertical="true"
            android:textColor="@color/black"/>
        <EditText
             android:textColorHint="@color/gray"
            android:inputType="text"
            android:background="@null"
            android:singleLine="true"
            android:ellipsize="end"
            android:layout_marginLeft="20dip"
            android:layout_toRightOf="@+id/design_tag_title_tv_info"
            android:id="@+id/design_tag_title_exp_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"/>
         
         </RelativeLayout>
         <ImageView
             android:visibility="visible"
             android:id="@+id/icard_tag_line3_info"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/icard_line"/>
       <RelativeLayout
           android:visibility="visible"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="@dimen/icard_top"
            android:paddingLeft="@dimen/icard_text_padding"
            android:background="@color/white"
            android:id="@+id/design_tag_desc_info">
        <TextView
            android:id="@+id/design_tag_desc_temp_info"
            android:layout_marginLeft="@dimen/icard_gv_spacing"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/design_tag_desc"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_centerVertical="true"
            android:textColor="@color/black"/>
        
        <EditText
             android:textColorHint="@color/gray"
            android:inputType="text"
            android:singleLine="true"
            android:ellipsize="end"
            android:background="@null"
            android:layout_toRightOf="@+id/design_tag_desc_temp_info"
            android:layout_marginLeft="20dip"
            android:id="@+id/design_tag_desc_exp_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_alignParentRight="true"/>
         </RelativeLayout>
        <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/icard_line"/>
        
        <RelativeLayout
            android:visibility="visible"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="@dimen/icard_top"
            android:paddingLeft="@dimen/icard_text_padding"
            android:background="@color/white"
            android:id="@+id/design_tag_video_map_rl_info">
        <TextView
            android:layout_marginLeft="@dimen/icard_gv_spacing"
            android:clickable="true"
            android:id="@+id/design_tag_video_map_title_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/icard_tag_video_link"
            android:textSize="@dimen/icard_text_size_middle"
            android:layout_centerVertical="true"
            android:textColor="@color/black"/>
        
        <EditText
            android:gravity="right"
            android:layout_toRightOf="@+id/design_tag_video_map_title_info"
            android:layout_marginLeft="50dip"
            android:id="@+id/design_tag_video_map_desc_info"
            android:layout_width="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:textSize="@dimen/icard_text_size_little"
            android:layout_marginRight="@dimen/icard_right"/>
     
         
         </RelativeLayout>
         
         <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/icard_line"/>

</LinearLayout>
